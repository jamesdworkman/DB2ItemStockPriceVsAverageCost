SELECT '10',T.ITNBR, T.PSTMAV, T.STPCS, T.AVCST, T.NUQOO
FROM amflibA.IMHIST T
JOIN (SELECT ITNBR, max(PSTMAV) AS PSTMAV2
FROM amflibA.IMHIST
GROUP BY ITNBR) TT ON T.ITNBR=TT.ITNBR AND T.PSTMAV=TT.PSTMAV2
WHERE PSTMAV > '2021-01-01 01:14:52' AND AVCST = 0 AND NUQOO > 0
UNION ALL 
SELECT '11',T.ITNBR, T.PSTMAV, T.STPCS, T.AVCST, T.NUQOO
FROM amflibB.IMHIST T
JOIN (SELECT ITNBR, max(PSTMAV) AS PSTMAV2
FROM amflibB.IMHIST
GROUP BY ITNBR) TT ON T.ITNBR=TT.ITNBR AND T.PSTMAV=TT.PSTMAV2
WHERE PSTMAV > '2021-01-01 01:14:52' AND AVCST = 0 AND NUQOO > 0
UNION ALL 
SELECT '5',T.ITNBR, T.PSTMAV, T.STPCS, T.AVCST, T.NUQOO
FROM amflib5.IMHIST T
JOIN (SELECT ITNBR, max(PSTMAV) AS PSTMAV2
FROM amflib5.IMHIST
GROUP BY ITNBR) TT ON T.ITNBR=TT.ITNBR AND T.PSTMAV=TT.PSTMAV2
WHERE PSTMAV > '2021-01-01 01:14:52' AND AVCST = 0 AND NUQOO > 0
UNION ALL 
SELECT '1',T.ITNBR, T.PSTMAV, T.STPCS, T.AVCST, T.NUQOO
FROM amflib1.IMHIST T
JOIN (SELECT ITNBR, max(PSTMAV) AS PSTMAV2
FROM amflib1.IMHIST
GROUP BY ITNBR) TT ON T.ITNBR=TT.ITNBR AND T.PSTMAV=TT.PSTMAV2
WHERE PSTMAV > '2021-01-01 01:14:52' AND AVCST = 0 AND NUQOO > 0
